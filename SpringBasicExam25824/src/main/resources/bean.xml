<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd" default-init-method="init" default-destroy-method="destroy">
    <!--  RentOfFlat    -->
    <bean id="rentOfOneBHKFlat" class="org.example.XMLBasedDI.ConstructorDIXmlBased.RentOfOneBHKFlat"/>
    <bean id="rentOfTwoBHKFlat" class="org.example.XMLBasedDI.ConstructorDIXmlBased.RentOfTwoBHKFlat"/>
    <bean id="rentOfThreeBHKFlat" class="org.example.XMLBasedDI.ConstructorDIXmlBased.RentOfThreeBHKFlat"/>
    <!-- Using constructor DI -->
    <bean id="rentOfFlat" class="org.example.XMLBasedDI.ConstructorDIXmlBased.RentOfFlat">
        <constructor-arg name="rentOfOneBHKFlat" ref="rentOfOneBHKFlat"/>
        <constructor-arg name="rentOfTwoBHKFlat" ref="rentOfTwoBHKFlat"/>
        <constructor-arg name="rentOfThreeBHKFlat" ref="rentOfThreeBHKFlat"/>
    </bean>
    <!--      SelfOwnedFlat  -->
    <bean id="selfOwnedOneBHKFlat" class="org.example.XMLBasedDI.SetterDIXmlBased.SelfOwnedOneBHKFlat"/>
    <bean id="selfOwnedTwoBHKFlat" class="org.example.XMLBasedDI.SetterDIXmlBased.SelfOwnedTwoBHKFlat"/>
    <bean id="selfOwnedThreeBHKFlat" class="org.example.XMLBasedDI.SetterDIXmlBased.SelfOwnedThreeBHKFlat"/>
    <bean id="selfOwnedFlat" class="org.example.XMLBasedDI.SetterDIXmlBased.SelfOwnedFlat" autowire="byName">
        <property name="selfOwnedOneBHKFlat" ref="selfOwnedOneBHKFlat"/>
        <property name="selfOwnedTwoBHKFlat" ref="selfOwnedTwoBHKFlat"/>
        <property name="selfOwnedThreeBHKFlat" ref="selfOwnedThreeBHKFlat"/>
    </bean>
</beans>